@model List<Annonce>
@{
    ViewData["Title"] = "Fake boncoin";
}


<form id="searchForm" asp-controller="Annonce" asp-action="SearchAjax" method="get" class="container">
    <div class="row justify-content-center align-items-center m-1 p-1">
        <input class="col-8 form-control" placeholder="Rechercher des annonces..." name="search" />
        <button type="submit" class="col-2 btn btn-secondary">Rechercher</button>
    </div>
</form>

<div id="result">

</div>
